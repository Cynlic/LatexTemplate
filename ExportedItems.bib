@article{hennion_lobjet_2018,
	title = {L’objet, la croyance et le sociologue: La sociologie de l’art comme oeuvre à faire},
	volume = {special issue},
	issn = {2110-6134 (electronic)},
	url = {http://ezproxy.lib.utexas.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=rih&AN=A1402933&site=ehost-live},
	abstract = {Less a sociology of music than a sociology arising from music. Writers from Michel de Certeau and Louis Marin to to William James and Étienne Souriau inspire other ways of perceiving the constitutive relationships between writing about music and the practice of music than the modes of necessary distancing or personal effusions. Musical experiences (composition, playing, the pleasures of the music-lover) are not simply where music is activated or where an external object is appropriated, they are the very locus of music. Therefore, although writing about these experiences cannot do justice to their process and its perpetual renewal, yet as is true for Certeau and the history of religion, writing helps to make them exist in a different way, to extend them, to supplement them and, conversely, such experiences can underpin writings on the sociology of music. This is illustrated in two examples: an experiment in comparing a text with the personal experience it sought to transcribe: learning how to sing, and an interview with a jazz improviser. The conception of improvisation expressed by the jazz musician is a startling echo of the powerful and original definition of Souriau's work in progress' which is discussed in conclusion. Correlations such as this demonstrate the necessity and the possibility of a sociology of art which, entirely unlike the prevailing scientism, would rank with creative works and, above all, with what they evoke, the worlds whose possibility they reveal. And is this evocation in the work not the province of the sociology of art?, Avec le recul, il me semble que j’ai moins fait de la sociologie de la musique qu’écrit une sociologie depuis la musique. Peut-on, en s’inspirant d’auteurs allant de Michel de Certeau et Louis Marin à William James et Étienne Souriau, reconnaître autrement que sur le mode de la distanciation obligée ou de l’épanchement personnel les rapports constitutifs qu’entretiennent une écriture sur la musique et une pratique de la musique ? Les expériences musicales (la création, le jeu, les plaisirs de l’amateur) sont le lieu même de la musique, elles ne sont pas la mise en oeuvre ou l’appropriation d’un objet extérieur. Dans ces conditions, si la mise par écrit de ces expériences ne peut rendre compte de leur procès, toujours à refaire, en revanche, comme c’est le cas pour l’histoire et la religion chez Certeau, l’écriture contribue à les faire exister autrement, à les prolonger, à les augmenter, tandis que réciproquement, de telles expériences ont soutenu l’écrit du sociologue de la musique que j’ai aussi été. Pour montrer cela, sur deux situations, je rapprocherai à titre expérimental une écriture et l’expérience qu’elle veut transcrire : l’apprentissage du chant – en m’appuyant sur mon propre cas – et un entretien avec un improvisateur de jazz. La conception de l’improvisation que nous livre ce jazzman fait étonnamment écho à la définition puissante et originale de l’« oeuvre à faire » que propose Souriau, sur laquelle conclut ce texte. De telles mises en rapport montrent la nécessité et la possibilité d’une sociologie de l’art qui, loin du scientisme ambiant, se place à la hauteur des oeuvres produites, et surtout de ce à quoi elles appellent, des mondes dont elles disent la possibilité. Cet appel de l’oeuvre ne concernerait-il pas la sociologie de l’art ?},
	pages = {136--153},
	number = {1},
	journaltitle = {Transposition, Hors série 1: Musique, histoire, sociétés},
	shortjournal = {Transposition},
	author = {Hennion, Antoine},
	urldate = {2018-01-01},
	date = {2018-01-01},
	langid = {french},
	keywords = {87: Music and related disciplines -- Social sciences. Sociology and cultural studies, sociology -- sociology as discipline -- sociology of art -- concept of work in progress},
	file = {Hennion - 2018 - L’objet, la croyance et le sociologue La sociolog.pdf:/Users/harrisonmontgomery/Zotero/storage/UW6QTKMQ/Hennion - 2018 - L’objet, la croyance et le sociologue La sociolog.pdf:application/pdf}
}

